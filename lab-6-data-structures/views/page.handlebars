<h1>{{data.title}}</h1>
{{#if data.slideshow}}
<div class="owl-carousel owl-theme">
{{#each data.slideshow}}
    <div class="item">
        <img class="img-fluid" src="{{this}}" alt="">
    </div>
{{/each}}      
</div>
{{/if}}

{{#if data.services}}
<div class="accordion" id="servicesAccordion">
  {{#each data.services}}
  <div class="accordion-item">
    <h2 class="accordion-header">
      <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapse_{{@index}}" aria-expanded="true" aria-controls="collapse_{{@index}}">
        {{this.title}}
      </button>
    </h2>
    <div id="collapse_{{@index}}" class="accordion-collapse collapse" data-bs-parent="#servicesAccordion">
      <div class="accordion-body">
        <p>{{this.intro}}</p>
        {{#each this.lists}}
        <li>{{this}}</li>
        {{/each}}
        <img class="img-fluid" src="{{this.image}}" alt="">
      </div>
    </div>
  </div>
  {{/each}} 
</div>
  {{/if}}